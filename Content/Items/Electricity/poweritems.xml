          <SpawnItem identifier="effect_none" spawnposition="ThisInventory" />
          <SpawnItem identifier="effect_none" spawnposition="ThisInventory" />
          <SpawnItem identifier="effect_none" spawnposition="ThisInventory" />
          <SpawnItem identifier="effect_none" spawnposition="ThisInventory" />
          <SpawnItem identifier="effect_none" spawnposition="ThisInventory" />
          <SpawnItem identifier="effect_none" spawnposition="ThisInventory" />
